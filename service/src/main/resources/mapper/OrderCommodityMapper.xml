<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.feidian.mapper.OrderCommodityMapper">
    <insert id="insertOrderCommodity">
        insert into order_commodity  (order_id, commodity_id, commodity_num)
        values (#{orderId}, #{commodityId}, #{commodityNum});
    </insert>


    <select id="findById" resultType="com.feidian.po.OrderCommodityPO">
        select * from order_commodity where id = #{id};
    </select>

</mapper>