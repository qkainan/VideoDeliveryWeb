<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.feidian.mapper.CommodityImageMapper">

    <insert id="insertCommodityImage">
        insert into commodity_image (commodity_id, image_url, image_status)
        values (#{commodityId}, #{imageUrl}, #{imageStatu})
    </insert>

    <select id="findByCommodityId" resultType="com.feidian.domain.CommodityImage">
        select *
        from commodity_image
        where id = #{commodityId};
    </select>

</mapper>